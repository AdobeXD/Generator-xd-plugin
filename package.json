{
  "name": "generator-xd-plugin",
  "version": "4.1.0",
  "description": "Yeoman generator for creating Adobe XD Plugin",
  "homepage": "http://github.com/adobexd/generator-xd-plugin",
  "author": {
    "name": "Adobe XD",
    "url": "https://adobexdplatform.com/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Adobe",
    "AdobeXD",
    "XD",
    "XDPlugin",
    "Plugin",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-xo": "^0.34.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^4.0.0"
  },
  "engines": {
    "npm": ">= 5.5.1"
  },
  "dependencies": {
    "ajv": "^7.0.4",
    "chalk": "^4.1.0",
    "lodash": "^4.17.20",
    "uuid": "^8.3.2",
    "yeoman-generator": "^4.13.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/adobexd/generator-xd-plugin"
  },
  "license": "Apache-2.0"
}
