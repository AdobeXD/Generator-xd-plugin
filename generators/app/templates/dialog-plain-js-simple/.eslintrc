{
	"extends": [
		"airbnb"
	],
	"plugins": [],
	"parserOptions": {},
	"env": {
		"browser": true
	},
	"globals": {},
	"rules": {
		"indent": ["error", 2],
		"no-unused-vars": "off",
		"no-console": "off",
    "import/no-unresolved": ["warn",
      {
        "ignore": [
          "application",
          "clipboard",
          "commands",
          "secenegraph",
          "uxp"
        ]
      }
    ],
    "no-param-reassign": ["error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "selection"
        ]
      }
    ]
	}
}
